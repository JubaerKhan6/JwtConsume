@model IEnumerable<JwtConsume.Models.RefreshTokens>

@{
    ViewData["Title"] = "GetTokens";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h1 class="text-light">Refresh Tokens</h1>

<table class="table text-light">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Token)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Expires)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Token)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expires)
            </td>
           
        </tr>
}
    </tbody>
</table>
<a asp-controller="Dashboard" asp-action="Index" class="btn btn-danger">Back</a>