@model JwtConsume.Models.UserEdit

@{
    ViewData["Title"] = "GetDetails";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h1 class="text-primary">Profile</h1>

<div>
    <h4 class="text-warning">@TempData["message"]</h4> 

    <hr />
    <dl class="row">
        <dt class = "col-sm-2 text-primary">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10 text-light">
            @TempData["Id"]
        </dd>
        <dt class="col-sm-2 text-primary">
            @Html.DisplayNameFor(model => model.Username)
        </dt>
        <dd class="col-sm-10 text-light">
            @TempData["message"]
        </dd>
        <dt class="col-sm-2 text-primary">
            @Html.DisplayNameFor(model => model.Role)
        </dt>
        <dd class="col-sm-10 text-light">
            @TempData["role"]
        </dd>
    </dl>
</div>
<div style="position:absolute; bottom:25%; left:5%;">
    
    <a asp-action="Index" class="btn btn-danger">Back</a>
</div>
