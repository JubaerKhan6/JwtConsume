<a asp-controller="Store" asp-action="GetHeroes" class=" btn btn-warning"> Browse Superheroes </a>
<a asp-controller="Store" asp-action="GetGames" class=" btn btn-warning"> Browse Video Games </a>
    <a asp-controller="Store" asp-action="GetMovies" class=" btn btn-warning"> Browse Movies </a>
